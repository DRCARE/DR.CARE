<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <com.tvnsoftware.drcare.customizeview.TopBarcodeView
        android:id="@+id/zxing_barcode_surface"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:zxing_framing_rect_height="@dimen/camera_qr_height"
        app:zxing_framing_rect_width="@dimen/camera_qr_width"
        />

    <com.journeyapps.barcodescanner.ViewfinderView
        android:id="@+id/zxing_viewfinder_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:zxing_possible_result_points="@color/zxing_custom_possible_result_points"
        app:zxing_result_view="@color/zxing_custom_result_view"
        app:zxing_viewfinder_laser="@color/zxing_custom_viewfinder_laser"
        app:zxing_viewfinder_mask="@color/zxing_custom_viewfinder_mask" />


    <ImageView
        android:layout_marginTop="@dimen/zing_barcode_margintop"
        android:layout_width="@dimen/camera_qr_width"
        android:layout_height="@dimen/camera_qr_height"
        android:layout_gravity="top|center_horizontal"
        android:scaleType="fitXY"
        android:background="@color/zxing_transparent"
        android:src="@drawable/bg_crop_frame" />

</merge>
